{
//  静态检查代码的语法和风格
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "no-alert": 0,
    "prefer-template": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "react/jsx-space-before-closing": 0
  },
  "plugins": [
    "react"
  ]
}